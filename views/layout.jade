!!!
html
	head
		title= title
		link(rel='stylesheet', href='/css/bootstrap.css')
		link(rel='stylesheet', href='/css/bootstrap-responsive.css')
		link(rel='stylesheet', href='/css/jquery-ui-1.8.24.min.css')
		link(rel='stylesheet', href='/css/darkstrap.css')
		link(rel='stylesheet', href='/css/default.css')
		script(type='text/javascript', src='/javascripts/jquery-1.8.3.min.js')
		script(type='text/javascript', src='/javascripts/jquery-ui-1.8.24.min.js')
		script(type='text/javascript', src='/javascripts/bootstrap.min.js')
		script(type='text/javascript')
			WebFontConfig = {
				google: { families: [ 'Oxygen', 'Varela Round' ] }
			};
			(function() {
				var wf = document.createElement('script');
				wf.src = ('https:' == document.location.protocol ? 'https' : 'http') +
				'://ajax.googleapis.com/ajax/libs/webfont/1/webfont.js';
				wf.type = 'text/javascript';
				wf.async = 'true';
				var s = document.getElementsByTagName('script')[0];
				s.parentNode.insertBefore(wf, s);
			})();
		block script
	body(data-spy='scroll', data-target='.subnav', data-offset='50')
		block content
		block extension
